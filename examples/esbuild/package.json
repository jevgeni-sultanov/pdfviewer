{
  "name": "pdfviewer-esbuild-example",
  "version": "1.0.0",
  "scripts": {
    "build": "esbuild ./index.ts --bundle --minify --sourcemap --outfile=dist/bundle.js",
    "build:cjs": "esbuild ./index.cjs --bundle --minify --sourcemap --outfile=dist/bundle.js",
    "build:esm": "esbuild ./index.mjs --bundle --minify --sourcemap --outfile=dist/bundle.js",
    "build:node": "esbuild ./index.ts --bundle --outfile=dist/bundle.js --platform=node --target=node8"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "pdf-generator-api-pdfviewer": "^1.0.0"
  }
}
